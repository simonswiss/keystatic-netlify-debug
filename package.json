{
  "name": "radenpioneer.blog",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "pnpm generate-icons && astro build",
    "check": "astro sync && pnpm dlx concurrently \"pnpm exec prettier --check --plugin-search-dir=. .\" \"astro check\" \"tsc --noEmit\"",
    "dev": "astro dev",
    "format": "pnpm exec prettier --write --plugin-search-dir=. .",
    "preview": "astro preview",
    "start": "pnpm build && pnpm preview",
    "generate-icons": "pwa-assets-generator --preset minimal public/logo.jpg"
  },
